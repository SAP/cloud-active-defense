{
  "filters": [
    {
      "decoy": {
        "key": ":status",
        "separator": "=",
        "value": ".*"
      },
      "detect": {
        "seek": {
          "inResponse":".*",
          "in": "header"
        },
        "alert": {
          "severity": "HIGH",
          "whenSeen": true
        }
      }
    }
  ]
}
